   url: \"http://taobao.com\",\n            contacts: \"马总\",\n            startDate: \"2019-08-28\",\n            endDate: \"2019-12-01\",\n            operator: \"大锤\"\n          },\n          {\n            location: \"A4\",\n            content: \"时尚潮流服饰\",\n            url: \"http://good.com\",\n            contacts: \"廖老板\",\n            startDate: \"2018-09-28\",\n            endDate: \"2019-01-10\",\n            operator: \"狗蛋\"\n          },\n          {\n            location: \"A3\",\n            content: \"人到中年秃不由己\",\n            url: \"http://666666.com\",\n            contacts: \"土豪\",\n            startDate: \"2019-03-28\",\n            endDate: \"2019-06-18\",\n            operator: \"翠花\"\n          },\n          {\n            location: \"A2\",\n            content: \"落魄前端炒粉\",\n            url: \"http://dddd.com\",\n            contacts: \"李老板\",\n            startDate: \"2019-02-13\",\n            endDate: \"2019-05-31\",\n            operator: \"小黄\"\n          },\n        ],\n      };\n    },\n    mounted() {\n      this.rowDrop();\n    },\n    methods: {\n      rowDrop() {\n        const tbody = document.querySelector(\".el-table__body-wrapper tbody\");\n        Sortable.create(tbody);\n      }\n    }\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.home {\n  width: 1200px;\n  margin: 0 auto;\n}\n</style>\n"]}]}